{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "downlevelIteration": true,
    "declaration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "paths": {
      "*": [
        "./source/typings/*"
      ],
      "type_aliases": [
        "./source/typings/type_aliases"
      ],
      "uniformat": [
        "./source/typings/uniformat"
      ]
    },
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "include": [
    "./source"
  ]
}
